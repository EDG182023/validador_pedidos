<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <!-- The project was targeting .NET Core 3.1 while using ImplicitUsings, a
         feature introduced in .NET 6. Updating the target framework resolves
         build errors related to this property. -->
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EPPlus" Version="5.8.3" />
    <PackageReference Include="Microsoft.AspNet.WebApi.Client" Version="5.2.9" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.5" />
  </ItemGroup>

</Project>
